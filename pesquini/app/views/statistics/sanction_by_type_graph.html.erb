<div class="graphics">


<% if @states[0] != "Todos" %>
  <% @states.unshift("Todos") %>
<% end %>

<%= form_tag("/statistics/sanction_by_type_graph", method: "get") do %>
  <%= select_tag 'state_', options_for_select(@states, :selected => params[:state_]) %>
<%= submit_tag "Confirmar", name: nil%>
<% end %>

</div>


<%= high_chart("sanjana2", @chart) %>
<h3 align = "center"><%= @titulo %></h3>
<div id="sanjana" style="height: 200px"></div>

